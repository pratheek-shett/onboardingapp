    function readExcel()
    {
        var fileInput = document.getElementById('excelFileInput');
        var file = fileInput.files[0];
        if (file) {
                var reader = new FileReader();
                reader.onload = function(e) {
                    var data = e.target.result;
                    var workbook = XLSX.read(data, {
                        type: 'binary'
                    });
                    var sheetName = workbook.SheetNames[0];
                    var sheet = workbook.Sheets[sheetName];
                    var jsonData = XLSX.utils.sheet_to_json(sheet, {
                        header: 1,
                        range: 1
                    });
                    // Define custom keys
                    var customKeys = ['Emp_NovigoEmailId__c', 'Emp_designation__c', 'Emp_dateOfJoining__c']; // Replace with your desired keys
                    // Convert array of arrays to array of objects with custom keys
                    var arrayOfObjects = jsonData.map(function(row) {
                        var obj = {};
                        customKeys.forEach(function(key, index) {
                            if(key=='Emp_dateOfJoining__c')
                            {
                                excelDate=row[index];
                                var jsDate = new Date((excelDate - 1) * 24 * 60 * 60 * 1000 + new Date(1899, 11, 30).getTime());
                                var day = jsDate.getDate().toString().padStart(2, '0');
                                var month = (jsDate.getMonth() + 1).toString().padStart(2, '0'); // Month is zero-based
                                var year = jsDate.getFullYear();
                                var formattedDate = year + '-' + month + '-' + day;
                                console.log(row[index]);
                                console.log(formattedDate);  
                                obj[key]=formattedDate;                              
                            }
                            else{
                                obj[key] = row[index];
                            }
                        });
                        return obj;
                    });
                    // Print the array of objects to the console in a readable format
                    console.log(arrayOfObjects);
                    const jsonRecords = JSON.stringify(arrayOfObjects);
                    Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.adminController.bulkUpload}',
                        jsonRecords,adminId,
                        function(result,event)
                        {
                            console.log(result);
                            location.reload();
                        });
                };
                reader.readAsBinaryString(file);
                generateErrorMsg("Data Successfully Imported. 🎉",'success');
            } else {
                generateErrorMsg("Please upload the Excel file! 🚫",'error');
            }
        }
    
    
