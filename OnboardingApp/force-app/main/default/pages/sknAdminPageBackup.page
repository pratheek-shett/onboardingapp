                let date = emp.Emp_lastModified__c.split(' ')[0];
 
                // Assuming you have a UTC time string like '9:33:05'
                var utcTimeString = time;
 
                // Extract hours, minutes, and seconds from the UTC time string
                var timeComponents = utcTimeString.split(':');
                var hours = parseInt(timeComponents[0]);
                var minutes = parseInt(timeComponents[1]);
                var seconds = parseInt(timeComponents[2]);
 
                var year = date.split('-')[0];
