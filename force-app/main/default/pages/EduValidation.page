        const enableInputFields = (parent)=>{
            const inputTags =parent.querySelectorAll('input');
                const selectTags = parent.querySelectorAll('select');
                const textTags = parent.querySelectorAll('textarea');
                inputTags.forEach((e)=>{
                    e.setAttribute('readonly','readonly');
                    e.classList.add('inputdisabled');
                });
                selectTags.forEach((e)=>{
                    e.disabled=true;
                });
                textTags.forEach((e)=>{
                    e.setAttribute('readonly','readonly');
                    e.classList.toggle('inputdisabled');
                });
        }
        const disableInputFields =(parent)=>{
            console.log(parent);
            const inputTags =parent.querySelectorAll('input');
                const selectTags = parent.querySelectorAll('select');
                const textTags = parent.querySelectorAll('textarea');
                inputTags.forEach((e)=>{
                    e.removeAttribute('readonly','readonly');
                    e.classList.remove('inputdisabled');
                });
                selectTags.forEach((e)=>{
                    e.disabled=false;
                });
                textTags.forEach((e)=>{
                    e.removeAttribute('readonly','readonly');
                    e.classList.toggle('inputdisabled');
                });
        }
        const toggleSaveAndEditButtons = (parent)=>{
