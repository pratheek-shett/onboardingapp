<apex:page showHeader="false" sidebar="false" standardStylesheets="false" applyHtmlTag="true" docType="html-5.0" controller="adminController">

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="preconnect" href="https://fonts.googleapis.com"/>
    <link rel="preconnect" href="https://fonts.gstatic.com" />
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700;800&display=swap" rel="stylesheet"/>
    <title>Admin Page</title>
    <style>
        *{
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        :root {
            --poppins: 'Poppins', sans-serif;
            --lato: 'Lato', sans-serif;
       
            --light: #F9F9F9;
            --blue: #3C91E6;
            --light-blue: #CFE8FF;
            --grey: #eee;
            --dark-grey: #AAAAAA;
            --dark: #342E37;
            --TransparentBlue:rgba(11,24,76,.85);
            --secondaryColor:#ca041b;
        }
        html,body{
            font-family: 'Montserrat', sans-serif;;
            font-weight: 400;
            overflow-x: hidden;
            font-size: 10px;
        }
        ::-webkit-scrollbar {
            width: 7px; /* width of the scrollbar */
          }
     
          /* Track */
          ::-webkit-scrollbar-track {
            background: var(--dark-grey); /* color of the track */
          }
     
          /* Handle */
          ::-webkit-scrollbar-thumb {
            background:var(--TransparentBlue); /* color of the handle */
            border-radius: 6px; /* roundness of the handle */
          }
          .main{
            width: 100vw;
            min-height: 100vh;
            position: absolute;
          }
          .navbar{
            width: 100vw;
            height: 7.2rem;
            background-color: var(--TransparentBlue);
            color: var(--light);
            display: grid;
            grid-template-columns: repeat(3,1fr);
            align-items: center;
            position:fixed;
            z-index: 1000;
            top:0;
 
            right: 0;
        }
        .navbar .left{
            grid-column:  1 / 3;
        }
        .navbar .left h1{
            font-size: 2.5rem;
            font-weight: 700;
            letter-spacing: -.5px;
            color: var(--light);
            text-align: center;
            text-transform: uppercase;
        }
        .navbar .right {
            grid-column: 3 / -1;
            display: flex;
            padding: 0rem 2rem;
            justify-content: flex-end;
        }
        .navbar .right .btn{
            width: 15rem;
            cursor: pointer;
            height: 4rem;
            font-weight: 700;
            letter-spacing: 1px;
            margin-right: 2rem;
            outline: none;
            border: none;
            border-radius: 5px;
            font-size: 1.6rem;
            color: var(--TransparentBlue);
            border: 1px solid var(--dark-grey);
            background-color: #fff;
            transition:all .3s linear;
        }
        .employee_info{
            max-width: 1200px;
            height: 70vh;
            background-color: var(--light);
            margin: 10rem auto;
            border-radius: 2rem;
            padding: 2rem 4rem;
            font-size: 1.6rem;
        }
        .button_group{
            display: flex;
            justify-content: flex-end;
            align-self: center;
        }
        .button_group .addemp{
            border: none;
            color: #fff;
            background-color: var(--TransparentBlue);
            padding: 1rem 4rem;
            font-size: 1.6rem;
            border-radius: 5px;
        }
        .employee_cards{
            width: 100%;
            display: grid;
            background-color:var(--TransparentBlue);
            color: #fff;
            grid-template-columns: repeat(11,1fr);
            padding: 3rem 1rem 2rem 3rem;
            border-radius: 10px;
            column-gap: 1rem;
            row-gap: 2rem;
            margin-top: 2rem ;
        }
        .input_div_1{
            grid-column:1/4 ;
        }
        .input_div_2{
            grid-column: 4/7;
        }
        .input_div_3{
            grid-column: 7/-1;
            display: flex;
            justify-content: space-between;
        }
        .progress_div{
            display: flex;
            flex-direction: column;
        }
        .progress_div label,
        .input_div label{
            display: block;
           
        }  
        .input_div input{
            width: 25rem;
            height: 4rem;
            font-size: 1.6rem;
            border-radius: 5px;
            border: none;
            outline: none;
            color: #999;
            padding-left: 1rem;
            margin-bottom: 2.5rem;
        }
        .input_div input:focus{
            border:2px solid var(--secondaryColor);
        }
        /*.progress_group{
            grid-column: 1/-1;
            display: flex;
            align-items: center;
            justify-content: space-between;
        }*/
        .emp_creation_pb,
        .emp_form_pb,
        .emp_verification_pb{
            width: 15rem;
            height:1rem;
            border-radius: 20px;
            background-color: orange;
        }
        .pdf{
            font-size: 1.6rem;
            /*grid-column: 7/-2;*/
            background-color: var(--secondaryColor);
            color: #fff;
            height: 4rem;
            cursor: pointer;
            width: 15rem;
            border: none;
            border-radius: 5px;
            margin-top: 3.9rem;
        }
        .generatePdf{
            font-size: 1.6rem;
            /*grid-column: 7/-2;*/
            background-color: var(--secondaryColor);
            color: #fff;
            height: 4rem;
            cursor: pointer;
            width: 15rem;
            border: none;
            border-radius: 5px;
            margin-top: 3.9rem;
        }
        /*.dropdown{
            grid-column: -2/-1;
        }*/
        .caret{
            margin-top: 3.9rem;
            font-size: 3rem;
            cursor: pointer;
            color: #eee;
            transition: all linear .4s;
        }
        .pb1{
            grid-column: 1/4;
        }
        .pb2{
            grid-column: 4/7;
        }
        .pb3{
            grid-column: 7/-2;
        }
        .emp_form_pb
        {
            background-color: red;
        }
        .emp_creation_pb{
            background-color: green;
        }
        .emp_verification_pb{
            background-color: red;
        }
        .employee_dropdown_div{
            grid-column: 1/-1;
            display: grid;
        }
        .rotate{
            transform: rotate(180deg);
        }
        .icons{
            display: flex;
            justify-content: space-between;
        }
        .deletebtn{
            background-color:var(--secondaryColor);
            border-radius:150rem ;
            border: none;
            color:#fff;
            width: 4rem;
            height: 4rem;
            cursor: pointer;
        }
        .copybtn{
            background-color:var(--secondaryColor);
            border-radius:150rem ;
            border: none;
            color:#fff;
            width: 4rem;
            height: 4rem;
            cursor: pointer;
        }
        .Popup{
            position:absolute;
            left: 0%;
            top:0%;
            background-color:rgba(255,255,255,255.5);
            width: 100vw;
            height: 100vh;
            z-index: 10;
            display: none;
            align-items: center;
            justify-content: center;
        }
        .verifyDocs{
            font-size: 1.6rem;
            /*grid-column: 7/-2;*/
            background-color: var(--secondaryColor);
            color: #fff;
            height: 4rem;
            cursor: pointer;
            width: 15rem;
            border: none;
            border-radius: 5px;
            margin-top: 3.9rem;
        }
        .credentials_div{
            display: flex;
            align-items: center;
            justify-content: space-between;
            background-color: var(--TransparentBlue);
            width: 70rem;
            height: 50rem;
            font-size: 1.6rem;
            color: #fff;
        }
        .credentials_div input{
            width: 25rem;
            height: 4rem;
            border: none;
            outline: none;
            border-radius: 5px;
            margin: 1rem 0rem;
        }
        .credentials_div button{
            padding: 1rem 3rem;
            border: none;
            color:#fff;
            border-radius: 5px;
            font-size: 1.6rem;
            margin-top: 2rem;
            background-color: var(--secondaryColor);
        }
        .credentials_div .credentials_content{
            width: 40%;
            height: 100%;
            font-size: 1.6rem;
            padding: 1rem 3rem;
            position: relative;
            background-color: var(--secondaryColor);
        }
        .credentials_content .cancel{
            position: absolute;
            cursor: pointer;
            right:1rem;
            top:1rem;
            font-size: 2rem;
        }
        .credentials_content .user{
            color:#fff;
            display: block;
            margin: 15rem auto;
            text-align: center  ;
            font-size: 6.4rem;
        }
        .credentials_div .credentials_form
        {
            padding:2rem 5rem;
        }
        .credentials_content_para{
            margin-top: 2rem;
        }
        .saveChecklist{
            font-size: 1.6rem;
            /*grid-column: 7/-2;*/
            background-color: var(--secondaryColor);
            color: #fff;
            height: 4rem;
            cursor: pointer;
            width: 15rem;
            border: none;
            border-radius: 5px;
            margin-top: 3.9rem;
        }

        /* message */
        .message {
                width: 50rem;
                top: 2rem;
                left: 50%;
                transform: translateX(-50%);
                position: absolute;
                height: 8rem;
                border-radius: 2px;
                z-index: 100000;
                background-color: var(--TransparentBlue);
                display: none;
                flex-direction: column;
            }
            
            .progressbar {
                height: .7rem;
                background-color: red;
                border-radius: 2px;
                animation-duration: 5s;
            }
            @keyframes progressbarAnimation {
                from {
                    width: 100%
                }
                to {
                    width: 0%;
                }
            }
            .errormsg {
                height: calc(100% - .7rem);
                color: white;
                font-size: 1.6rem;
                font-weight: 600;
                display: flex;
                align-items: center;
                padding: 0 3rem;
                justify-content: space-between;
            }
            
            .cancel {
                cursor: pointer;
            }
    </style>
</head>
