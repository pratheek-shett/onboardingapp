                const inputTags = parent.querySelectorAll('input');
                const selectTags = parent.querySelectorAll('select');
                const textTags = parent.querySelectorAll('textarea');
                inputTags.forEach((e) => {
                    e.setAttribute('readonly', 'readonly');
                    e.classList.add('inputdisabled');
                });
                selectTags.forEach((e) => {
                    e.disabled = true;
                });
                textTags.forEach((e) => {
                    e.setAttribute('readonly', 'readonly');
                    e.classList.toggle('inputdisabled');
                });
            }
