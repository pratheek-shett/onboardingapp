                const selectTags = parent.querySelectorAll('select');
                const textTags = parent.querySelectorAll('textarea');
                inputTags.forEach((e) => {
                    e.removeAttribute('readonly', 'readonly');
                    e.classList.remove('inputdisabled');
                });
                selectTags.forEach((e) => {
                    e.disabled = false;
                });
                textTags.forEach((e) => {
                    e.removeAttribute('readonly', 'readonly');
                    e.classList.toggle('inputdisabled');
                });
            }
