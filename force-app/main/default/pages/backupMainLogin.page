     <script>
        const form = document.querySelector('.employee-form');
        const message = document.querySelector('.message');
        const pdfForUser = document.querySelector('.pdfForUser');
        const eventmsg = document.querySelector('.messagep');
        const pdfButton = document.querySelector('.pdfBtn');
        const loginButton = document.querySelector('.loginBtn');
        let employeeId='';

        form.addEventListener('submit' , (e)=>{
            const username = document.querySelector('.username').value;
            const password = document.querySelector('.password').value;
            console.log("username",username);
            console.log("password", password);

            e.preventDefault();
            eventmsg.innerHTML ='';

            Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.EO_BackupLogin.getEmpId}',username,password,
            function (result,event){
                employeeId = result;
                console.log("employee id", employeeId);
            });

            Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.EO_BackupLogin.check}',username,password,
            function (result,event){
                let loginType = result;
                eventmsg.innerHTML = loginType;
                if(result.includes('Success'))
                {
                    loginType = result.split(' ')[0];
                    eventmsg.innerHTML = loginType;
                    const ss= result.split(' ')[1];
                    const ref ='/apex/mainPage?id='+ss;
                    window.location.href  = ref;
                    message.style.backgroundColor='green';
                }
                else if(result.includes('Admin'))
                {
                    loginType = result.split(' ')[0];
                    eventmsg.innerHTML = loginType;
                    const ss= result.split(' ')[1];
                    const ref ='/apex/adminPage?id='+ss;
                    window.location.href  = ref;
                    message.style.backgroundColor='green';
                }
                else if(result.includes('submitted'))
                {
                    loginType = result;
                    eventmsg.innerHTML = loginType;
                    const ss = result;
                    message.style.backgroundColor='red';
                    loginButton.style.display = 'none';
                    pdfButton.style.display = 'block';
                }
            }); 
            setTimeout(function(){
                message.style.display='block';
            },1000);
            setTimeout(function(){
                // Your code to be executed after 5 seconds
                message.style.display='none';
            }, 5000);
        }); 

        pdfButton.addEventListener('click' , (e)=>{
            e.preventDefault();
            console.log("empId inside pdf", employeeId);
            Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.EO_BackupLogin.pdfDataForUser}',employeeId,
            function(result,event){
                console.log(result);
                printPDF(result);
            });
        });

        document.querySelector('.cancel').addEventListener('click',(e)=>{
            e.preventDefault();
            message.style.display='none';
        });

