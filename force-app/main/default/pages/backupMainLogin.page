        function printkycInfoTable(empKycInfo)
        {
            var kyctemp = `
                                <table class="kycTable" style="margin-left:auto; margin-right:auto; margin-top:2rem; width:90%; align:center; border:1px solid black; border-collapse: collapse; padding-bottom: 10px; padding-left:3px; text-align:left;">
                                    <caption style="background-color:#d3d3d3; color:black; font-size: 22px;">KYC Details</caption>                    
                                    <tr>
                                        <td class="rowHeading" style="font-weight:bold; border:1px solid black; border-collapse: collapse; padding-top: 10px; padding-bottom: 10px; padding-left:3px;">KYC Document Type</td>
                                        <td class="rowHeading" style="font-weight:bold; border:1px solid black; border-collapse: collapse; padding-top: 10px; padding-bottom: 10px; padding-left:3px;">Name as on Document</td>
                                        <td class="rowHeading" style="font-weight:bold; border:1px solid black; border-collapse: collapse; padding-top: 10px; padding-bottom: 10px; padding-left:3px;">Document No.</td>
                                        <td class="rowHeading" style="font-weight:bold; border:1px solid black; border-collapse: collapse; padding-top: 10px; padding-bottom: 10px; padding-left:3px;">Remark if any</td>
                                    </tr>`;
            if(empKycInfo)
            {
                empKycInfo.forEach(info =>{
                kyctemp += `<tr>
                                <td class="rowHeading" style="font-weight:bold; border:1px solid black; border-collapse: collapse; padding-top: 10px; padding-bottom: 10px; padding-left:3px;">${info.Kyc_documentType__c ? info.Kyc_documentType__c : ''}</td>
                                <td class="rowValue" style="border:1px solid black; border-collapse: collapse; padding-top: 10px; padding-bottom: 10px; padding-left:3px;">${info.Kyc_nameOnDocument__c ? info.Kyc_nameOnDocument__c : ''}</td>
                                <td class="rowValue" style="border:1px solid black; border-collapse: collapse; padding-top: 10px; padding-bottom: 10px; padding-left:3px;">${info.Kyc_documentNumber__c ? info.Kyc_documentNumber__c : ''}</td>
                                <td class="rowValue" style="border:1px solid black; border-collapse: collapse; padding-top: 10px; padding-bottom: 10px; padding-left:3px;">${info.Kyc_remarks__c ? info.Kyc_remarks__c : ''}</td>
                            </tr>`;
                });
            }

            kyctemp += `</table></div><br/>`
            return kyctemp;                  
        }
