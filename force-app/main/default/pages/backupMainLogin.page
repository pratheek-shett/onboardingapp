                color: #999;
                display: block;
                font-weight: 500;
            }
            .employee-form input{
                width: 30rem;
                display: block;
                height: 4.5rem;
                font-size: 1.6rem;
                border: none;
                border-radius: 5px;
                outline: none;
                margin-bottom: 1rem;
                padding-left: 10px;
            }
            .employee-form input:focus{
                border: 2px solid var(--secondaryColor);
                padding-left: 10px;
            }
            .buttonClass {
                display: flex;
                justify-content: space-between;
            }
            .loginBtn{
                width: 15rem;
                height: 4rem;
                background-color: #ca041b;
                border-radius: 5px;
                outline: none;
                border: none;
                color:#ddd;
                font-weight: 700;
                font-size: 1.6rem;
                margin-top: 2rem;
                cursor: pointer;
                display:block;
            }
            .pdfBtn{
                width: 15rem;
                height: 4rem;
                background-color: #ca041b;
                border-radius: 5px;
                outline: none;
                border: none;
                color:#ddd;
                font-weight: 700;
                font-size: 1.6rem;
                margin-top: 2rem;
                cursor: pointer;
                display: none;
                /* margin-left: 5px; */
            }
            .message{
                width: 50rem;
                height:7rem;
                background-color: rgba(167, 20, 20, 0.9);
                color: #ddd;
                position: absolute;
                top:5%;
                left:50%;
                transform: translateX(-50%);
